loglog([2078, 4064, 7840, 16832, 20928, 27328, 34592], [160, 270, 460, 800, 950, 1480, 1660], 'o-', 'linewidth', 2, 'Color', OrangeRed2, 'MarkerFaceColor', OrangeRed2);
%% curve_adaptive_inexact_newton
loglog([2078, 4064, 7840, 16832, 20928, 27328, 34592], [60, 90, 120, 150, 170, 190, 210], '^-', 'linewidth', 2, 'Color', dark_green, 'MarkerFaceColor', dark_green);
