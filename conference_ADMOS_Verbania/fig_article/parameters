plot([2078, 4064, 7840, 16832, 20928, 27328, 34592], [16, 27, 46, 80, 95, 148, 166], 'o-', 'linewidth', 2, 'Color', OrangeRed2, 'MarkerFaceColor', OrangeRed2);
%% curve_adaptive_inexact_newton
plot([2078, 4064, 7840, 16832, 20928, 27328, 34592], [6, 9, 12, 15, 17, 19, 21], '^-', 'linewidth', 2, 'Color', dark_green, 'MarkerFaceColor', dark_green);
