%%%%%Matlab_plot number algebraic iterations during newton steps %%%%%
gamma_alg=0.1 
gammalin=0.1
Hmax_0.15 
maxit=10

exact_newton_iter=15
inexact_newton_iter=46
adaptive_inexact_newton_iter=12

temp_plot_exact_res = [200, 20, 40, 40, 40, 40, 40, 60, 60, 60, 60, 60, 60, 60, 40]


temp_plot_inexact_res = repmat(10, 46, 1)'


temp_plot_adapt_inexact = repmat(10,12,1)'
figure;
loglog(1 : exact_newton_iter, temp_plot_exact_res , 's-', 'linewidth', 2, 'Color', RoyalBlue1, 'MarkerFaceColor', RoyalBlue1); 
hold on;
%% curve_inexact_newton
loglog(1 : inexact_newton_iter, temp_plot_inexact_res, 'o-', 'linewidth', 2, 'Color', OrangeRed2, 'MarkerFaceColor', OrangeRed2);
%% curve_adaptive_inexact_newton
loglog(1 : adaptive_inexact_newton_iter, temp_plot_adapt_inexact, '^-', 'linewidth', 2, 'Color', dark_green, 'MarkerFaceColor', dark_green);
xlabel('Newton iteration', 'FontSize', 15);
ylabel('Number of algebraic solver iterations', 'FontSize', 15)      
axis([1 100 1 1000]);
h_legend = legend('exact ', 'inexact ', 'adapt inexact ');
set(h_legend, 'FontSize', 11);


