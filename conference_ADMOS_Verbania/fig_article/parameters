temp_plot_exact_res = [200, 20, 40, 40, 40, 40, 40, 60, 60, 60, 60, 60, 60, 60, 40]


temp_plot_inexact_res = repmat(10, 46, 1)'


temp_plot_adapt_inexact = [9, 48, 33, 18, 18, 9, 9, 9, 9, 6, 6, 6] 



%%curve_exact_newton
loglog(1:15, temp_plot_exact_res, 's-', 'linewidth', 2, 'Color', RoyalBlue1, 'MarkerFaceColor', RoyalBlue1); 
hold on;
%% curve_inexact_newton
loglog(1:46, temp_plot_inexact_res, 'o-', 'linewidth', 2, 'Color', OrangeRed2, 'MarkerFaceColor', OrangeRed2);
%% curve_adaptive_inexact_newton
loglog(1 : 12, temp_plot_adapt_inexact, '^-', 'linewidth', 2, 'Color', dark_green, 'MarkerFaceColor', dark_green);
xlabel('Newton iteration', 'FontSize', 15);
ylabel('Number of algebraic solver iterations', 'FontSize', 15);                   
h_legend = legend('exact ', 'inexact ', 'adapt inexact ');
set(h_legend, 'FontSize', 11);





