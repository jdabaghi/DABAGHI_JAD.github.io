%----------------------------------------------------------------------------------------
%	INTRODUCTION
%----------------------------------------------------------------------------------------
% Framework: 
% Parametrized nonlinear parabolic problems
% Common procedure:
% Reduced-Basis with Empirical Interpolation Methods (RB-EIM). 
% Bottlneck: 
% Offline stage
% Solving a significant number of nonlinear full-order model problems. 
% Eventually prohibitive EIM costs.
% Purpose: 
% Reduce the offline cost with good online accuracy.
% Key idea: 
% Progressive enrichment of both the EIM approximation 
% and the RB space.
  The  purpose of PREIM~\cite{preim} is  to  \red{reduce  the  offline  costs} of nonlinear 
  parabolic reduced order models with accurate RB  approximations  in  the  online  stage.
  The  key  idea  is  a  \red{progressive  enrichment}  of  both
the EIM approximation and the RB space, in contrast to the standard approach where the EIM
approximation  and  the  RB  space  are  built  separately.
PREIM  uses  high-fidelity  computations whenever available and RB computations otherwise.
Another key feature of each PREIM iteration
is to select twice the parameter in a greedy fashion, 
the second selection being made after computing
the high-fidelity solution for the firstly selected value of the parameter.
% \vspace{1cm}
%----------------------------------------------------------------------------------------
%	OBJECTIVES
%----------------------------------------------------------------------------------------

\color{Black} % DarkSlateGray color for the rest of the content
\begin{tcolorbox}[enhanced, breakable,colback=white,frame style={left color=white!25!black,
right color=blue!75!black},
width=\dimexpr0.45\textwidth+18mm\relax,enlarge left by=0mm, title = \huge Model problem, 
bottomrule=3mm, leftrule=1mm, toptitle = 3mm, bottomtitle = 3mm, center title]
% \section*{Model problem}

For $\mu\in \mathcal P^{\rm tr}$, we consider the nonlinear heat transfer equation
%
\begin{equation}\label{HTE}
  \left\{
  \begin{alignedat}{2}
    \frac{\partial u_\mu}{\partial t}
   - \nabla \cdot \big((\kappa_0 + \red{\Gamma(\mu,u_\mu)}) \nabla u_\mu\big)& 
   = f, &&\mathrm{in}\ [0,T]\times\Omega , \\
  -\big(\kappa_0 + \Gamma(\mu,u_\mu)\big) \frac{\partial u_\mu}{\partial n}&
  = \phi_e,\ \ \ \ &&\mathrm{on}\  [0,T]\times\partial\Omega , \\
  u_\mu(t=0,\cdot)&= u_0(\cdot),\quad && \mathrm{in}\  \Omega,
  \end{alignedat}
  \right.
  \end{equation}
  with the standard uniform ellipticity assumption
$\beta_1 \leq \kappa_0 + \Gamma(\mu,z) \leq \beta_2$
and $0<\beta_1<\beta_2<\infty$.
\end{tcolorbox}

%----------------------------------------------------------------------------------------
%	SPACE DISCRETIZATION
%----------------------------------------------------------------------------------------

\begin{tcolorbox}[enhanced, breakable,colback=white,frame style={left color=white!25!black,
right color=blue!75!black},
width=\dimexpr0.45\textwidth+18mm\relax,enlarge left by=0mm, title = \huge Standard RB-EIM, 
bottomrule=3mm, leftrule=1mm, toptitle = 3mm, bottomtitle = 3mm, center title]
For the RB approximation
$
\mathbf{\hat{u}}^{k}_{\mu} =\sum_{n=1}^N \hat{u}^k_{\mu,n}
{\bem \theta}_n,
$
, the space-time discretized PDE~\eqref{HTE} in reduced formulation reads
\begin{align*}\label{rbmat0}
(\mathbf M  + \Delta t^k {\mathbf A_0}) \mathbf{\hat{u}}^{k}_{\mu} 
 = \Delta t^k  \mathbf f^k 
 + \mathbf M \mathbf{\hat{u}}^{k-1}_{\mu}
 -\Delta t^k  \mathbf g(\mathbf{\hat{u}}^{k-1}_{\mu}),
\end{align*}
with \begin{equation} \label{eq:G_bad}
{\mathbf g}(\mathbf{\hat{u}}^{k-1}_{\mu})
= \bigg( \sum_{n=1}^N\hat u^{k-1}_{\mu,n} \int_\Omega\Gamma\bigg(\mu,\sum_{n'=1}^N \hat u^{k-1}_{\mu,n'}\theta_{n'}\bigg)
\nabla \theta_n\cdot\nabla\theta_p \bigg)_{1\leq p \leq N }
\end{equation}
We introduce $
\gamma(\mu,k,x) := \Gamma(\mu,u_\mu^k(x))$ 
and seek a separated approximation of $\gamma$ 
\begin{equation}\label{eq:eim}
\gamma_M(\mu,k,x)  := \sum_{j=1}^M \varphi_{\mu,j}^{k}  q_j(x),
\end{equation}
In the standard RB-EIM, the EIM approximation~\eqref{eq:eim}
and the RB space $X_N = \mathrm{span}\{(\theta_n)_{1\leq n\leq N}\}$
are built separately.
% \white{nada}
\end{tcolorbox}
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\begin{tcolorbox}[enhanced, breakable,colback=white,frame style={left color=white!25!black,
right color=blue!75!black},
width=\dimexpr0.45\textwidth+18mm\relax,enlarge left by=0mm, title =\huge PREIM, 
bottomrule=3mm, leftrule=1mm, toptitle = 3mm, bottomtitle = 3mm, center title]
% \vspace{0.7cm}
\begin{minipage}[b]{\textwidth}
\centering
\includegraphics[width=\textwidth]{docs/POST/scheme.png}
\end{minipage}
% \vspace{0.1cm}
\tcblower
% \vspace{1cm}
We introduce 
\begin{equation}\label{preim_func}
\bar u^{m,k}_\mu := \left\{
 \begin{alignedat}{2}
  &u^k_\mu &&\quad\text{if }\mu \in 
 \mathcal P_{m}^\textsc{HF}, \\
  &\hat{u}^{m,k}_\mu &&\quad \text{otherwise},
 \end{alignedat}
 \right .
\end{equation}
and the nonlinear function at iteration $m$
\begin{equation} \label{eq:bar_gamma_m}
 \bar \gamma^m(\mu,k,x) := \Gamma(\mu,\bar u_\mu^{m,k}(x)).
\end{equation}
PREIM has \green{\bfseries two goals}: 
\begin{itemize}
 \item produce a set of RB functions $(\theta_n^m)_{1\leq n\leq N^m}$; 
\item produce a rank-$m$ approximation of the nonlinearity~\eqref{eq:bar_gamma_m}
in the form
\begin{equation*} \label{eq:gamma_m_m}
\bar\gamma_m^m(\mu,k,x):=
\sum_{j=1}^m(\bar\varphi^m)_{\mu,j}^{k}\bar q_j(x).
\end{equation*}
\end{itemize}
\green{\bfseries three main ingredients}:
\begin{itemize}
 \item $\mathcal{P}^\textsc{HF}_m\subset \mathcal P^{\rm tr}$: Set that collects 
 the previously computed HF trajectories
 \item $\mathcal{X}_m$: Set of interpolation points $(\bar x_i)_{1\leq i\leq m}$
 \item $\mathcal{Q}_m$: Set of interpolation functions $(\bar q_j)_{1\leq j\leq m}$
 \end{itemize}
\green{\bfseries three main steps} (the detailed algorithm appears in~\cite{preim}):
\begin{itemize}
 \item select a pair
$$
(\mu_m, k_m) 
\in \underset{(\mu',k')\in \mathcal{P}^\mathrm{tr} \times \overline{\mathbb K}^\mathrm{tr}}
{\operatorname{argmax}} \|\Gamma\big(\mu',\bar u^{k'}_{\mu'}(\cdot)\big)
-\bar\gamma_{[\mathcal{P}^\textsc{HF}_{\mathrm{in}},\mathcal{X}_{m-1},\mathcal{Q}_{m-1}]}^{m-1}(\mu',k',\cdot)\|_{\ell^\infty(\Omega^\mathrm{tr})}.
$$
\item compute $\mathcal S_{\mathrm{out}} = (u^k_{\mu_m})_{k\in \overline{\mathbb K}^\mathrm{tr}}$ 
and update the pair
$$
(\bar\mu_m,\bar k_m)
\in \underset{(\mu',k')\in
{\mathcal{P}_{\mathrm{out}}^\textsc{HF}} \times \overline{\mathbb K}^\mathrm{tr}
}{\operatorname{argmax}}\|\Gamma\big(\mu', u^{k'}_{\mu'}(\cdot)\big)
-\bar\gamma_{[\mathcal{P}^\textsc{HF}_{\mathrm{in}},\mathcal{X}_{m-1},\mathcal{Q}_{m-1}]}^{m-1}(\mu',k',\cdot)\|_{\ell^\infty(\Omega^\mathrm{tr})}.
$$
\item update the reduced basis 
$$\Xi := \mathrm{POD}(\tilde{\mathcal S},\epsilon_\textsc{pod})$$
with $\Theta:= (\theta_n^m)_{1\leq n\leq N^m}$ and $\tilde{\mathcal S_{\mathrm{out}}}:= 
(u - \Pi_{\mathrm{span}(\Theta)} u)_{u\in \mathcal{S}_{\mathrm{out}}}$
\end{itemize}
and \green{\bfseries three accuracy criteria}:
\begin{itemize}
 \item $\epsilon_\textsc{pod}$ is the truncation threshold for the POD-based RB construction.
 \item $\epsilon_\textsc{eim}$ is threshold for the approximation of the nonlinearity.
 \item $\epsilon_\textsc{eb}$ is threshold for the RB approximation.
\end{itemize}
\end{tcolorbox}
